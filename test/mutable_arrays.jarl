arr := [1, 1, 0, 0, 0]
arr[2] = arr[0] + arr[1]
arr[3] = arr[1] + arr[2]
arr[4] = arr[2] + arr[3]
assert arr[4] == 5, "arrays should be mutable"

arr1 := [1, 2]
arr2 := arr1
arr2[0] = 3
assert arr1[0] == 1 and arr1[1] == 2, "arrays should follow copy semantics"

arr1 = [[1]]
arr2 = arr1
arr1[0][0] = 2
assert arr1[0][0] == 2 and arr2[0][0] == 1, "copy semantics should be deep"
